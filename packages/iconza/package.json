{
  "name": "iconza",
  "version": "2.0.0",
  "private": false,
  "description": "A modern, accessible icon set with original brand colors, full TypeScript support, and delightful animations.",
  "homepage": "https://iconza.vercel.app",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "icons",
    "react",
    "typescript",
    "svg",
    "icon-library",
    "icon-set",
    "accessible-icons",
    "animated-icons",
    "brand-icons"
  ],
  "scripts": {
    "start": "tsup src/index.ts --watch",
    "build": "tsup src/index.ts",
    "prepublishOnly": "pnpm build"
  },
  "sideEffects": false,
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "react-dom": {
      "optional": false
    }
  },
  "devDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.0.0"
  },
  "license": "MIT"
}
